<query id="deleteTextyleApi" action="delete">
    <tables>
        <table name="textyle_api" />
    </tables>
    <conditions>
        <condition operation="equal" column="module_srl" var="module_srl" filter="number" notnull="notnull" />
        <condition operation="equal" column="api_srl" var="api_srl" filter="number" notnull="notnull" pipe="and" />
    </conditions>
</query>
