<query id="getTextyleSubscriptionMinPublishDate" action="select">
    <tables>
        <table name="textyle_subscription" />
    </tables>
    <columns>
        <column name="min(publish_date)" alias="publish_date" />
    </columns>

    <conditions>
        <condition operation="equal" column="module_srl" var="module_srl" filter="number" notnull="notnull" />
    </conditions>
</query>
